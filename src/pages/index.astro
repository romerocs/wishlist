---
import { url } from "../utilities/vars";
import { getUser } from '../utilities/auth';

const user = await getUser(Astro.request);

const isLoggedIn = !(user == null);

import Layout from "../layouts/Layout.astro";
import AuthAppPrototype from "../components/AuthAppPrototype.vue";
---

<Layout title="Wishlist" user={user}>
  <h1>Homepage</h1>

  <AuthAppPrototype client:only="vue" url={url} isLoggedIn={isLoggedIn} />
</Layout>

<style>
  .app > * {
    gap: 0;
  }
</style>
